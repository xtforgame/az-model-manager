// ============== start model: {{ name | capitalize }} ==============
type {{ name | capitalize }}CreationAttributes = {
{% for column in model.columns -%}
{%- if column[1].autoIncrement -%}
{%- else -%}
{% render 'column.liquid', name: column[0], column: column[1], optionMark: '?' %}
{%- endif -%}
{%- endfor -%} };

type {{ name | capitalize }}Attributes = Overwrite<{{ name | capitalize }}CreationAttributes, {
{% for column in model.columns -%}
{%- if column[1].autoIncrement -%}
{% render 'column.liquid', name: column[0], column: column[1], optionMark: '' %}
{%- endif -%}
{%- endfor -%} }>;

type {{ name | capitalize }}I = {{ name | capitalize }}Attributes & {
{% render 'timestamp-columns.liquid', model: model -%}

{% for column in model.columns -%}
{%- if column[1].type[0] == "hasMany" or column[1].type[0] == "hasOne"  -%}
{% render 'associations.liquid', model: model -%}
{%- endif -%}
{%- endfor -%} };
// ============== end model: {{ name | capitalize }} ==============

